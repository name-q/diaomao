{
  "name": "diaomao",
  "version": "1.0.0",
  "description": "掉毛 - AI代码审查服务",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^3",
    "egg-scripts": "^2",
    "egg-cors": "^2",
    "axios": "^1.6.0",
    "js-yaml": "^4.1.0",
    "tiktoken": "^1.0.10"
  },
  "devDependencies": {
    "egg-bin": "^6",
    "egg-ci": "^2",
    "egg-mock": "^5",
    "eslint": "^8",
    "eslint-config-egg": "^12"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-diaomao",
    "stop": "egg-scripts stop --title=egg-server-diaomao",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autofix": "npm run lint -- --fix"
  }
}
